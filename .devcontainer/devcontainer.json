{
  "name": "META-F4_D4",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "workspaceFolder": "/workspaces/SIME",
  "postCreateCommand": "pip install --no-cache-dir -r requirements.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "github.copilot",
        "ms-azuretools.vscode-docker",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspaces/SIME,type=bind,consistency=cached"
  ],
  "remoteUser": "vscode"
}